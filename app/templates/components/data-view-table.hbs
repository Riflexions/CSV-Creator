<br>
<div class="table-responsive">
<table class="table table-bordered table-striped table-hover">
    <thead>
        <tr>
            {{#each fieldList.fields as |field|}}
            <th>
                {{field}}
            </th>
            {{/each}}
        </tr>
    </thead>
    <tbody>
        {{#each dataList.records as |rec|}}
        <tr>
            {{#each rec as |r|}}
            <td>
               {{r}}
            </td>
            {{/each}}
        </tr>
        {{else}}
        <p>No Elements</p>
        {{/each}}
    </tbody>
</table>
</div>
<br>