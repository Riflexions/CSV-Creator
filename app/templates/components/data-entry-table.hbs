<br>
<table class="table table-bordered table-striped table-hover table-responsive">
    <thead>
        <tr>
            {{#each fieldList.fields as |field|}}
            <th>
                {{field}}
            </th>
            {{/each}}
        </tr>
    </thead>
    <tbody>
        {{#each dataList.records as |rec index1|}}
        <tr>
            {{#each rec as |recordValue index|}}
            <td>
                {{entry-input recordValue=recordValue index=index index1=index1 deleteRecs=(action 'deleteRecs') editRecs=(action 'editRecs')}}
            </td>
            {{/each}}
        </tr>
        {{/each}}
        <p>No Elements</p>
        <tr>
            {{#each fieldList.fields as |field|}}
            <td>
                {{input type="text" placeholder="Enter" class="form-control input-md"}}
            </td>
            {{/each}}
        </tr>
    </tbody>
</table>
<div class="text-center">
    <a id="add_row" class="btn btn-default text-center">Add Row</a>
</div>
